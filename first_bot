@bot.event
async def on_member_join(ctx):  # –≤—ã–¥–∞—á–∞ —Ä–æ–ª–∏ –∑–∞—à–µ–¥—à–µ–º—É —É—á–∞—Å—Ç–Ω–∏–∫—É
    role = discord.utils.get(ctx.guild.roles, name='–ü–æ—Ç–µ—Ä—è—à–∫–∞')
    await ctx.add_roles(role)


@bot.event
async def on_member_remove(member):  # –ø—Ä–æ—â–∞–Ω–∏–µ —Å –≤—ã—à–µ–¥—à–∏–º —É—á–∞—Å—Ç–Ω–∏–∫–æ–º —Å–µ—Ä–≤–µ—Ä–∞
    global chann  # id –∫–∞–Ω–∞–ª–∞, –≤ –∫–æ—Ç–æ—Ä—ã–π –æ—Ç–ø—Ä–∞–≤–ª—è–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
    my_channel = discord.utils.get(bot.get_all_channels(), id=chann)
    await my_channel.send(f'{member} –ø–æ–∫–∏–Ω—É–ª –Ω–∞—Å. –ü–æ–∂–µ–ª–∞–µ–º –µ–º—É —É–¥–∞—á–∏!')
    # await my_channel.send(f'{member}, {member.id} –≤—ã—à–µ–ª —Å —Å–µ—Ä–≤–µ—Ä–∞.')


