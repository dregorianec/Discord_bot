@bot.command()
async def avatar(ctx, member: discord.Member = None):  # –ü–æ–ª—É—á–µ–Ω–∏–µ –∞–≤–∞—Ç–∞—Ä–∫–∏ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
    '''
    –í—ã–∑—ã–≤–∞–µ—Ç –∞–≤–∞—Ç–∞—Ä
    '''
    user = member

    embed = discord.Embed(title=f'–ê–≤–∞—Ç–∞—Ä –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è {user}', color=user.color)
    # embed = discord.Embed(color=0xff9900, title='Random Fox')  # –°–æ–∑–¥–∞–Ω–∏–µ Embed'a
    print(user.avatar_url)
    embed.set_image(url=user.avatar_url)  # –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫–∞—Ä—Ç–∏–Ω–∫—É Embed'a
    await ctx.send(embed=embed)  # –û—Ç–ø—Ä–∞–≤–ª—è–µ–º Embed


@bot.command()
async def kiss(ctx, user: discord.Member = None):  # –ü–æ—Ü–µ–ª—É–π
    kao = kaomoji.Kaomoji()
    try:
        if user.mention != ctx.message.author.mention and user.mention:
            await ctx.send(f'{ctx.message.author.mention} –ø–æ—Ü–µ–ª–æ–≤–∞–ª(–∞) {user.mention}! {kao.random_kaomoji()}')
        elif user.mention == ctx.message.author.mention and user.mention:
            await ctx.send(f'–£ {ctx.message.author.mention} –ø–æ—Ü–µ–ª–æ–≤–∞–ª(–∞) —Å–∞–º(–∞) —Å–µ–±—è! {kao.random_kaomoji()}')
    except AttributeError:
        await ctx.send(f'{ctx.message.author.mention}, —Ç—ã –Ω–µ –º–æ–∂–µ—à—å –ø–æ—Ü–µ–ª–æ–≤–∞—Ç—å –Ω–∏—á—Ç–æ!')


@bot.command()
async def con(ctx, messageId: int):  # –∏—Å–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Ä–∞—Å–∫–ª–∞–¥–∫–∏ –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã(–ê–ª–∞, –ø–µ—Ä–µ–¥–µ–ª–∞—Ç—å –≤ —Å–≤–æ–µ)
    s = ''
    slovar = {'`': '—ë', '~': '–Å', 'q': '–π', 'Q': '–ô',
              'w': '—Ü', 'W': '–¶', 'e': '—É', 'E': '–£',
              'r': '–∫', 'R': '–ö', 't': '–µ', 'T': '–ï',
              'y': '–Ω', 'Y': '–ù', 'u': '–≥', 'U': '–ì',
              'i': '—à', 'I': '–®', 'o': '—â', 'O': '–©',
              'p': '–∑', 'P': '–ó', '[': '—Ö', '{': '–•',
              ']': '—ä', '}': '–™', 'a': '—Ñ', 'A': '–§',
              's': '—ã', 'S': '–´', 'd': '–≤', 'D': '–í',
              'f': '–∞', 'F': '–ê', 'g': '–ø', 'G': '–ü',
              'h': '—Ä', 'H': '–†', 'j': '–æ', 'J': '–û',
              'k': '–ª', 'K': '–õ', 'l': '–¥', 'L': '–î',
              ';': '–∂', ':': '–ñ', "'": '—ç', '"': '–≠',
              "z": '—è', 'Z': '–Ø', 'x': '—á', 'X': '–ß',
              'c': '—Å', 'C': '–°', 'v': '–º', 'V': '–ú',
              'b': '–∏', 'B': '–ò', 'n': '—Ç', 'N': '–¢',
              'm': '—å', 'M': '–¨', ',': '–±', '<': '–ë',
              '.': '—é', '>': '–Æ', '/': '.', '?': ',',
              ' ': ' '
              }

    message = await ctx.guild.get_channel(ctx.channel.id).fetch_message(messageId)
    for i in message.clean_content:
        if i not in slovar:
            s += i
        else:
            s += slovar[i]

    author = "–ê–≤—Ç–æ—Ä —Å–æ–æ–±—â–µ–Ω–∏—è: " + str(message.author) + "\n"
    clean_content = "–Ω–∞–ø–∏—Å–∞–ª: \n" + str(message.clean_content) + "\n\n"
    system_content = "—Ö–æ—Ç–µ–ª –Ω–∞–ø–∏—Å–∞—Ç—å: \n" + str(s) + "\n\n"
    created_at = "–°–æ–∑–¥–∞–Ω–Ω–æ: " + str(message.created_at) + "\n"
    symbol = ['1', '2', '3', '4', '5', '6', '7', '8', '9', '0', 'A', 'B', 'C', 'D', 'F']
    a1 = choice(symbol)
    a2 = choice(symbol)
    b1 = choice(symbol)
    b2 = choice(symbol)
    c1 = choice(symbol)
    c2 = choice(symbol)
    expert = '0x' + a1 + a2 + b1 + b2 + c1 + c2
    cvet = '–¶–≤–µ—Ç: ' + expert + "\n"
    mess = discord.Embed(title="–ü—É–Ω—Ç–æ —Å–≤–∏—á–µ—Ä", description=clean_content + system_content + created_at + cvet,
                         colour=int(expert, 16))
    mess.set_footer(text=f'–°–æ–∑–¥–∞–Ω–Ω–æ: {message.author}', icon_url=str(message.author.avatar_url))
    await ctx.send(embed=mess)


# –ò–≤–µ–Ω—Ç—ã~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
@bot.event
async def on_message(message):  # –µ—â–µ –æ–¥–Ω–∞ –≤—Ä–µ–º–µ–Ω–Ω–∞—è –ø—Ä–∏—è—Ç–Ω–∞—è –≤–µ—â—å
    await bot.wait_until_ready()
    await bot.process_commands(message)
    author = message.author
    if message.content == '–ü—Ä–∏–≤–µ—Ç, –†–∏–≥':
        await message.channel.send('–î–æ–±—Ä–æ–µ —É—Ç—Ä–æ, {}!'.format(author))


