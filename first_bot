            await ctx.send(f'{ctx.message.author.mention} –ø–æ–≥–ª–∞–¥–∏–ª(–∞) –ø–æ –≥–æ–ª–æ–≤–µ {user.mention}! {kao.random_kaomoji()}')
        elif user.mention == ctx.message.author.mention and user.mention:
            await ctx.send(f'–£ {ctx.message.author.mention} —Å–∞–º–æ–ø–æ–≥–ª–∞–∂–∏–≤–∞–Ω–∏–µ! {kao.random_kaomoji()}')
    except AttributeError:
        await ctx.send(f'{ctx.message.author.mention}, –ø–æ–∑–≤–æ–ª—å –ø–æ–≥–ª–∞–¥–∏—Ç—å —Ç–µ–±—è –ø–æ –≥–æ–ª–æ–≤–µ^^')


